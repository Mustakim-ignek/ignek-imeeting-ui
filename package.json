{
  "name": "ignek-imeeting-ui",
  "version": "1.0.0",
  "description": "Ui library for resuable components in imeeting project",
  "license": "MIT",
  "repository": "Mustakim/ignek-imeeting-ui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "ui",
    "clayui",
    "react-component",
    "custom"
  ],
  "author": {
    "name": "Mustakim",
    "email": "kazi.mustakim@ignek.com"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --css-modules false --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --css-modules false --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^5.0.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@clayui/css": "^3.62.1",
    "@clayui/icon": "^3.56.0",
    "@clayui/modal": "^3.58.1",
    "@clayui/pagination": "^3.62.0",
    "@clayui/progress-bar": "^3.56.0",
    "formik": "^2.2.9",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.7.1",
    "react-dropzone": "^14.2.1",
    "react-images-uploading": "^3.1.6",
    "react-multiselect-checkboxes": "^0.1.1",
    "react-table": "^7.8.0",
    "rollup-plugin-styles": "^4.0.0"
  }
  
}
